<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userinterface.SignUpScreenController">
   <children>
      <TextField fx:id="emailField" layoutX="300.0" layoutY="129.0" promptText="E-mail Address">
         <font>
            <Font size="12.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordField" layoutX="300.0" layoutY="170.0" promptText="Password">
         <font>
            <Font size="12.0" />
         </font>
      </PasswordField>
      <Button layoutX="346.0" layoutY="210.0" mnemonicParsing="false" onAction="#signUpPressed" text="Sign Up">
         <font>
            <Font size="12.0" />
         </font></Button>
      <Label fx:id="warningLabel" layoutX="150.0" layoutY="92.0" prefHeight="15.0" prefWidth="450.0" textFill="RED" AnchorPane.topAnchor="100.0">
         <font>
            <Font size="12.0" />
         </font>
      </Label>
      <Hyperlink fx:id="cancelButton" layoutX="353.0" layoutY="244.0" onAction="#cancelButtonPressed" text="Cancel" underline="true">
         <font>
            <Font size="12.0" />
         </font>
      </Hyperlink>
   </children>
</AnchorPane>
