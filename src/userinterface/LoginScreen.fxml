<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.paint.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="750.0" styleClass="root" stylesheets="@JMetroLightTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userinterface.LoginScreenController">
   <children>
      <Label fx:id="warningLabel" alignment="CENTER" layoutX="149.0" layoutY="155.0" prefHeight="15.0" prefWidth="450.0" textAlignment="CENTER" textFill="RED" AnchorPane.topAnchor="155.0">
         <font>
            <Font size="12.0" />
         </font></Label>
      <TextField fx:id="emailField" layoutX="285.0" layoutY="180.0" prefHeight="30.0" prefWidth="180.0" promptText="E-mail address" stylesheets="@JMetroLightTheme.css">
         <font>
            <Font size="14.0" />
         </font></TextField>
      <PasswordField fx:id="passwordField" layoutX="285.0" layoutY="215.0" prefHeight="30.0" prefWidth="180.0" promptText="Password" stylesheets="@JMetroLightTheme.css">
         <font>
            <Font size="14.0" />
         </font></PasswordField>
      <Button fx:id="loginButton" alignment="CENTER" defaultButton="true" layoutX="285.0" layoutY="250.0" mnemonicParsing="false" onAction="#loginPressed" prefHeight="30.0" prefWidth="180.0" stylesheets="@JMetroLightTheme.css" text="Login">
         <font>
            <Font size="12.0" />
         </font></Button>
      <Label layoutX="40.0" layoutY="40.0" styleClass="header" stylesheets="@JMetroLightTheme.css" text="Emotion Engine">
         <font>
            <Font name="Segoe UI" size="30.0" />
         </font>
      </Label>
      <Button fx:id="signUpButton" alignment="CENTER" layoutX="285.0" layoutY="290.0" mnemonicParsing="false" onAction="#signUpPressed" prefHeight="30.0" prefWidth="89.0" style="-fx-font-size: 11;" text="Sign Up">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button fx:id="forgotPasswordButton" alignment="CENTER" layoutX="375.0" layoutY="290.0" mnemonicParsing="false" onAction="#forgotPasswordPressed" prefHeight="30.0" prefWidth="89.0" style="-fx-font-size: 11;" text="Forgot?">
         <font>
            <Font size="11.0" />
         </font></Button>
      <Button fx:id="closeButton" layoutX="715.0" layoutY="7.0" mnemonicParsing="false" styleClass="window-button" text="x" />
      <Button fx:id="minimizeButton" layoutX="695.0" layoutY="7.0" mnemonicParsing="false" styleClass="window-button" text="-" />
      <ProgressIndicator fx:id="progressIndicator" layoutX="465.0" layoutY="251.0" prefHeight="30.0" prefWidth="20.0" />
   </children>
</AnchorPane>
