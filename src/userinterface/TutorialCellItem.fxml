<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" maxHeight="180.0" maxWidth="400.0" prefHeight="180.0" prefWidth="400.0" styleClass="background" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@JMetroLightTheme.css" />
    </stylesheets>
   <children>
      <HBox fx:id="mainHBox" prefHeight="160.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <ImageView fx:id="imageView" fitHeight="140.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true">
               <viewport>
                  <Rectangle2D />
               </viewport>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ImageView>
            <VBox HBox.hgrow="ALWAYS">
               <children>
                  <Hyperlink fx:id="hyperLink" alignment="TOP_LEFT" prefHeight="24.0" style="-fx-text-fill: #008287;" text="Hyperlink" underline="true" VBox.vgrow="NEVER" />
                  <TextFlow fx:id="textFlow" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="NEVER">
                     <VBox.margin>
                        <Insets left="5.0" />
                     </VBox.margin></TextFlow>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
