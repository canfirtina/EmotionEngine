<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="750.0" styleClass="background" stylesheets="@JMetroLightTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userinterface.ForgotPasswordScreenController">
   <children>
      <Label layoutX="40.0" layoutY="40.0" styleClass="header" stylesheets="@JMetroLightTheme.css" text="Forgot Password">
         <font>
            <Font name="Segoe UI" size="30.0" />
         </font>
      </Label>
      <Button layoutX="375.0" layoutY="215.0" mnemonicParsing="false" onAction="#cancelButtonPressed" prefHeight="30.0" prefWidth="89.0" style="-fx-font-size: 11;" text="Cancel">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button layoutX="285.0" layoutY="215.0" mnemonicParsing="false" onAction="#sendPressed" prefHeight="30.0" prefWidth="89.0" style="-fx-font-size: 11;" stylesheets="@JMetroLightTheme.css" text="Send">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <TextField fx:id="emailField" layoutX="285.0" layoutY="180.0" prefHeight="30.0" prefWidth="180.0" promptText="E-mail Address">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button fx:id="closeButton" layoutX="715.0" layoutY="7.0" mnemonicParsing="false" styleClass="window-button" text="x" />
      <Button fx:id="minimizeButton" layoutX="695.0" layoutY="7.0" mnemonicParsing="false" styleClass="window-button" text="-" />
   </children>
</AnchorPane>
